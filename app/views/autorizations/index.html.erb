<p id="notice"><%= notice %></p>

<h1>Listing Autorizations</h1>

<table>
  <thead>
    <tr>
      <th>Number</th>
      <th>Password</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @autorizations.each do |autorization| %>
      <tr>
        <td><%= autorization.number %></td>
        <td><%= autorization.password %></td>
        <td><%= link_to 'Show', autorization %></td>
        <td><%= link_to 'Edit', edit_autorization_path(autorization) %></td>
        <td><%= link_to 'Destroy', autorization, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Autorization', new_autorization_path %>
